# Options for the "find images" command:
image_find_predicates = \
	-name '.svn' -prune \
	-o \
	  -type d \
	  \( -name '[a-z][a-z]' -o -name '[a-z][a-z]_[A-Z][A-Z]' \) \
	  $(image_lang_exclude) \
	  -prune \
	-o \
	\( \
	-name '*.png' -o \
	-name '*.jpg' -o \
	-name '*.mng' -o \
	-name '*.xcf' -o \
	-name '*.gif'    \
	\) -print


# languages we want to exclude from installing images
image_lang_exclude = \
	$(shell echo $(strip $(ALL_LINGUAS)) | \
	        sed -e '/^$$/d; \
		        s/ / -o -name /g; \
			s/^/! \\( -name /; \
			s/$$/ \\)/ ' \
	)

html_files_find_predicates = \
	-type f -name '*.*' \
	-print

